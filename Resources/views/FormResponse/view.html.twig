{# Dywee\ModuleBundle\Resources\View\CustomForm\add.html.twig #}

{% extends '@DyweeCMSBundle/admin.html.twig' %}

{% block metaTitle %}
    Coup d'oeil sur une réponse de formulaire {{ parent() }}
{% endblock %}

{% block body %}
    <div class="box box-default">
        <div class="box-header">
            <h1 class="box-title">Réponse à "{{ customForm.name }}"</h1>
        </div>
        <div class="box-body">
            {{ form(response) }}
        </div>
    </div>

{% endblock %}
